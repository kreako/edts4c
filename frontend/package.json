  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "lint:fix": "eslint --fix --ext .js,.vue ./",
    "test": "folio test/playwright --param browserName=firefox --param screenshotOnFailure",
    "test:codegen": "playwright codegen http://0.0.0.0:8080/",
    "test:e2e": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress open\"",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress run\""
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
